{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "template": " \uf871",
          "foreground": "#000000",
          "background": "#AE101B",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "template": " {{ .Path }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b8",
          "background": "#AE101B",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "powerline_symbol": "\ue0b8",
          "background": "#3A3A3A",
          "type": "text",
          "style": "accordion"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "trailing_diamond": "\ue0b0",
          "template": "{{ .HEAD }} {{ if .Working.Changed }}{{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#E94E50> \uf046 {{ .Staging.String }}</>{{ end }} ",
          "foreground": "#BABABA",
          "background": "#3A3A3A",
          "type": "git",
          "style": "diamond",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#3A3A3A{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#C5E478{{ end }}",
            "{{ if gt .Ahead 0 }}#F08650{{ end }}",
            "{{ if gt .Behind 0 }}#F08650{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#BABABA>\ue0b2</>",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "round",
            "threshold": 0
          },
          "template": " {{ .FormattedMs }} ",
          "foreground": "#000000",
          "background": "#BABABA",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<parentBackground,#AE101B>\ue0bc</>",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04"
          },
          "template": " {{ .CurrentDate | date .Format }} ",
          "foreground": "#000000",
          "background": "#AE101B",
          "type": "time",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#AE101B>\ue0b4</>",
          "type": "text",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "‚ùØ ",
          "foreground": "#2BDE5F",
          "type": "exit",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
